<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RichToolBar">
        <attr name="title_text" format="string"/>
        <attr name="right_text" format="string"/>
        <attr name="left_img_src" format="reference"/>
        <attr name="left_back_visible" format="boolean"/>
        <attr name="right_img_src" format="reference"/>
        <attr name="is_divider_visible" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="VolumeBar">
        <attr name="default_volume" format="integer"/>
    </declare-styleable>

</resources>